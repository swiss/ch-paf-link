    prov:wasGeneratedBy <https://politics.ld.admin.ch/parliamentary-affair/23.4022/registration-activity>.

# The consultation creation activity creates the consultation identifier entity and the consultation content entity.
<https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/creation-activity> a chpaf:OCoCreationActivity;
    prov:startedAtTime "2023-10-03T22:00:00.0000000Z";
