    schema:name "Title of the consultation";
    prov:wasGeneratedBy <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/creation-activity>.

# the consultation registration activity is used to register the consultation in the it system that handles the process (the registration date is not the same as the real start of the consultation).
<https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/registration-activity> a chpaf:OCoRegistrationActivity;
    prov:used
        <https://politics.ld.admin.ch/parliamentary-affair/23.4022>,
        <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157>,
        <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/entity>;
    prov:startedAtTime "2023-10-03T22:00:00.0000000Z";
    prov:wasInformedBy <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/creation-activity>. 

# The consultation activity represents the actual consultation process, with start and end date, all the information in the entities and the responsible office as creator.
<https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/activity> a chpaf:OCoActivity;
    prov:qualifiedAssociation <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/creator>;
    prov:used 
        <https://politics.ld.admin.ch/parliamentary-affair/23.4022>,
        <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157>,
        <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/entity>;
        
    prov:wasInformedBy <https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/registration-activity>;
    prov:endedAtTime "2023-10-19T22:00:00.0000000Z";
    prov:startedAtTime "2023-10-05T22:00:00.0000000Z".

<https://politics.ld.admin.ch/office-consultation/OCo_2023.2157/creator> a prov:Association;
    prov:agent <https://ld.admin.ch/office/III.1.4>;
    prov:hadRole chpaf:OCoCreator.

